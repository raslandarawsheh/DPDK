                        <params>
                                <static_args> --master_rc ' ' --checksum 'UDP'  --first_case ' ' --msg-size 64 --device '/dev/mst/mt4103_pci_cr0'  --fwd-mode sw</static_args>
                        </params>
                </cmd>
        </case>

